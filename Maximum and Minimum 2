//Input :N=8, 2,4,6,8,10,12,14,18.
//Output : Min = 2, Max =18
def find_min_max(arr):
    if not arr:
        return None, None
    
    min_val = arr[0]
    max_val = arr[-1]
    
    return min_val, max_val

# Example usage
N = 8
a = [2, 4, 6, 8, 10, 12, 14, 18]
min_val, max_val = find_min_max(a)

print(f"Min = {min_val}, Max = {max_val}")

